<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"

    android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="match_parent"

            android:layout_height="match_parent">


            <View
                android:id="@+id/rectangle_81"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="-1dp"
                android:layout_marginLeft="-1dp"
                android:layout_marginTop="0dp"
                android:alpha="0.40000000596046"
                android:background="@drawable/rectangle_81_shape" />

            <View
                android:id="@+id/ellipse_26"
                android:layout_width="match_parent"
                android:layout_height="225dp"
                android:layout_centerHorizontal="true"
                android:layout_marginStart="-30dp"
                android:layout_marginLeft="-30dp"
                android:layout_marginTop="566dp"
                android:background="@drawable/ellipse_26_shape" />



            <ImageView android:id="@+id/popup_home"
                android:background="@drawable/ic_home_btn"
                android:contentDescription="__page_dashboard__ic_home_24px"
                android:layout_marginLeft="14dp"
                android:layout_marginStart="14dp"
                android:layout_marginTop="683.5dp"
                android:layout_width="45dp"
                android:layout_height="46dp" />

            <TextView
                android:id="@+id/home"
                android:layout_width="54dp"
                android:layout_height="30dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="730dp"
                android:text="@string/home_string"
                android:textColor="@color/monthly_target_color"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/today_tasks"
                android:layout_width="100dp"
                android:layout_height="41dp"
                android:layout_marginLeft="60dp"
                android:layout_marginTop="664dp"
                android:text="@string/today_tasks_string"
                android:textColor="@color/monthly_target_color"
                android:textStyle="bold" />



            <ImageView android:id="@+id/popup_today"
                android:background="@drawable/ic_today_task"
                android:contentDescription="__page_today_task__ic_work_24px"
                android:layout_marginLeft="80.1162109375dp"
                android:layout_marginStart="80.1162109375dp"
                android:layout_marginTop="617.93994140625dp"
                android:layout_width="45dp"
                android:layout_height="45dp" />
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true">
                <ImageButton
                    android:id="@+id/popup_dissmiss"
                    android:layout_width="60dp"
                    android:layout_height="match_parent"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/rectangle_75_ek1_shape"/>
                <TextView android:id="@+id/x"
                    android:textSize="17sp"
                    android:textAlignment="center"
                    android:textColor="@color/today_task_color"
                    android:layout_centerHorizontal="true"
                    android:layout_width="15dp"
                    android:layout_height="25dp"
                    android:layout_marginTop="8dp"
                    android:textStyle="bold"
                    android:text="X"
                    android:gravity="center_horizontal" />

            </RelativeLayout>
            <!--<View
                android:id="@+id/rectangle_75_ek1"
                android:layout_width="77dp"
                android:layout_height="38dp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="713dp"
                android:background="@drawable/rectangle_75_ek1_shape" />-->

            <TextView
                android:id="@+id/nearby_stores_ek1"
                android:layout_width="100dp"
                android:layout_height="30dp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="638dp"
                android:text="@string/nearby_stores_ek1_string"
                android:textColor="@color/employee_name_color"
                android:textStyle="bold" />



            <ImageView android:id="@+id/popup_nearby"
                android:background="@drawable/ic_location_btn"
                android:contentDescription="__page_nearby_stores__ic_pin_drop_24px"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="592.28564453125dp"
                android:layout_width="45dp"
                android:layout_height="45dp" />



            <TextView
                android:id="@+id/add_customer"
                android:layout_width="100dp"
                android:layout_height="21dp"
                android:layout_alignParentRight="true"
                android:layout_marginRight="40dp"
                android:layout_marginTop="664dp"
                android:text="@string/add_customer_string"
                android:textColor="@color/monthly_target_color"
                android:textStyle="bold" />



            <ImageView android:id="@+id/popup_addcustomer"
                android:background="@drawable/ic_add_customer"
                android:contentDescription="__page_adding_a_customer__ic_person_add_24px"
                android:layout_alignParentRight="true"
                android:layout_marginRight="78dp"
                android:layout_marginTop="617.6083984375dp"
                android:layout_width="45dp"
                android:layout_height="45dp" />

            <TextView
                android:id="@+id/complaints"
                android:layout_width="79dp"
                android:layout_height="29dp"
                android:textAlignment="center"
                android:layout_alignParentRight="true"
                android:layout_marginTop="736dp"
                android:text="@string/complaints_string"
                android:textColor="@color/monthly_target_color"
                android:textStyle="bold"
                android:gravity="center_horizontal" />

            <ImageView android:id="@+id/popup_complan"
                android:background="@drawable/ic_complaints"
                android:contentDescription="__page_complaints__ic_speaker_notes_24px"
                android:layout_alignParentRight="true"
                android:layout_marginRight="22dp"
                android:layout_marginTop="690dp"
                android:layout_width="45dp"
                android:layout_height="45dp" />
        </RelativeLayout>

</RelativeLayout>